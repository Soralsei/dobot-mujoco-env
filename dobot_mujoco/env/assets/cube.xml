<mujoco model="cube">
    <compiler angle="radian" meshdir="meshes/" />
    <option timestep="1e-3" solver="Newton" integrator="Euler" />

    <asset>
        <material name="cube_mat" rgba="0.25 0.17 0.12 1" />
    </asset>

    <worldbody>

        <body name="cube_body" pos="0 0 0">
            <joint type="free" />
            <geom name="cube_geom" size="0.02 0.02 0.02" type="box" />
        </body>

    </worldbody>

</mujoco>